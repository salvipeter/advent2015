(defun adv01 (str)
  (loop with level = 0
        for c across str do
          (incf level (if (char= c #\() 1 -1))
        finally (return level)))

(defun adv01b (str)
  (loop with level = 0
        for c across str and pos upfrom 1 do
          (incf level (if (char= c #\() 1 -1))
        until (< level 0)
        finally (return pos)))

